{
  "name": "web-pilot",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f .env.dev next dev --turbopack",
    "build": "env-cmd -f .env.prod next build",
    "start": "env-cmd -f .env.prod next start",
    "lint": "next lint",
    "prisma:dev": "env-cmd -f .env.dev prisma",
    "prisma:prod": "env-cmd -f .env.prod prisma",
    "prisma:seed": "env-cmd -f .env.dev npx tsx prisma/seed.ts",
    "prisma:reset": "env-cmd -f .env.dev prisma migrate reset",
    "prisma:migrate": "env-cmd -f .env.dev prisma migrate dev"
  },
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "@prisma/extension-accelerate": "^2.0.2",
    "argon2": "^0.43.1",
    "next": "15.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@prisma/internals": "^6.13.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "colors": "^1.4.0",
    "env-cmd": "^10.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "prisma": "^6.13.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.3",
    "typescript": "^5"
  }
}
